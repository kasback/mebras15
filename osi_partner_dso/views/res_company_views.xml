<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data>
        <record id="res_company_extend_form_dso" model="ir.ui.view">
            <field name="name">res.company.extend.form.dso</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook" position="inside">
                    <page string="DSO">
                        <group>
                            <field name="dso_global_desired"/>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
