<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data  noupdate="1">

        <!-- Sequences for paiement record -->
        <record id="seq_paiement_record" model="ir.sequence">
            <field name="name">Paiement Record</field>
            <field name="code">paiement.record</field>
             <field name="prefix">P/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">5</field>
        </record>

        <record id="seq_paiement_cash" model="ir.sequence">
            <field name="name">Cash Sequence</field>
            <field name="code">paiement.cash.client</field>
             <field name="prefix">Cash/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">5</field>
        </record>
    </data>
</odoo>
